<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.monitor.mapper.SysEmployeeMapper">

    <resultMap id="BaseResultMap" type="com.example.monitor.domain.SysEmployee">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="employee" column="employee" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,employee,create_time
    </sql>
</mapper>
